  <step>
    <name>Gasto mensal por parlamentar</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>localhost</connection>
    <sql>SELECT parlamentar.nome as Parlamentar, data.mes as Mes, data.ano as Ano, 
uf.cota as Cota, SUM(valorDespesa) as Despesa
FROM pdi_db.despesa
INNER JOIN parlamentar ON parlamentar.idParlamentar = despesa.idParlamentar
INNER JOIN uf ON uf.idUF = despesa.idUF
INNER JOIN data ON data.idData = despesa.idData 
GROUP BY despesa.idParlamentar, data.mes
ORDER BY data.mes, data.ano;
</sql>
    <limit>0</limit>
    <lookup />
